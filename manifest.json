{
  "name": "BringBackTheBird",
  "description": "Sick of Twitter's new rebrand? This extension changes it back to that iconic bird!",
  "homepage_url": "https://github.com/humboldt123/BringBackTheBird",
  "version": "1.0.2",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png",
      "512": "icon512.png"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/**"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "bringbackthebird@vivime.info",
      "strict_min_version": "42.0"
    }
  }
}
